%% compare is < and > open is {  and }
var g = 5;
if (g < 5) {
  %% EPICCCCC
  %% USING THIS
  print("g < 5");
} elif (g > 5) {
  print ("g > 5");
} else { %% looks weird otherwise
  print ("g == 5");
}

foreach (var name of array) {
  %% do something
  %% lets make it foreach so its EASY
  %% ok
  print(name);
}

%% no no so basically foreach is for looping through arrray
for [i, 0, array.length, 1] {
  let item = array[i];
  print(item);
}

