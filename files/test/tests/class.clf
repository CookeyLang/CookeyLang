class Bacon {
  construct(name) {
    this.name = name;
  }

  greet() {
    printLine("Hewwo, I am " + this.name);
  }

  eat() {
    printLine("Crunch Crunch, " + this.name + " got eaten!");
  }

  cook() {
    printLine(this.name + " is getting cooked! Sizzle Sizzle");
  }
}


var widdlebacon = Bacon("happy widdle bacon");
widdlebacon.greet();
widdlebacon.cook();
widdlebacon.eat();
print("Bacon is edible? ");

var bacon2 = Bacon("old name").construct("new name");
bacon2.greet();