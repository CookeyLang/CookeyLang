class Cat {
    construct(name) {
        this.name = name;
    }

    eat(food) {
        printLine(this.name + " eats " + food);
    }

    drink(liquid) {
        printLine(this.name + " drinks some " + liquid);
    }
}

var jo = Cat("Jo");
jo.eat("catfood");
jo.drink("water");

if(jo.name == "Jo") printLine("phew we found Jo");
el printLine("oh noes we lost Jo");