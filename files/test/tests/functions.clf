%% RECURSION
function isEven(num) {
  if (num == 0) ret true;
  el if (num == 1) ret false;
  el if (num < 0) ret isEven(-num);
  el ret isEven(num - 2);
}

%% hehe faster
function isOdd(num) {
  ret num % 2 == 1;
}

%% pog
function magic(num) {
  ret num ^ 2;
}

%% POG
printLine(isEven(magic(3)));
printLine(isOdd(magic(3)));